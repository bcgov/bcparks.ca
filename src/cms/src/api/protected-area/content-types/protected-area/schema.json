{
  "kind": "collectionType",
  "collectionName": "protected_areas",
  "info": {
    "singularName": "protected-area",
    "pluralName": "protected-areas",
    "displayName": "Protected-area",
    "name": "protected-area",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true,
    "privateAttributes": [
      "search_text",
      "search_rank"
    ]
  },
  "attributes": {
    "orcs": {
      "type": "integer",
      "unique": true,
      "required": true
    },
    "protectedAreaName": {
      "type": "string"
    },
    "totalArea": {
      "type": "decimal"
    },
    "uplandArea": {
      "type": "decimal"
    },
    "marineArea": {
      "type": "decimal"
    },
    "marineProtectedArea": {
      "type": "string",
      "maxLength": 1,
      "minLength": 1,
      "regex": "[Y,N]"
    },
    "type": {
      "type": "string"
    },
    "class": {
      "type": "string"
    },
    "establishedDate": {
      "type": "date"
    },
    "repealedDate": {
      "type": "date"
    },
    "status": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "oldUrl": {
      "type": "string"
    },
    "typeCode": {
      "type": "enumeration",
      "enum": [
        "PK",
        "PA",
        "RA",
        "ER",
        "CS"
      ]
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "mapZoom": {
      "type": "integer"
    },
    "isFogZone": {
      "type": "boolean"
    },
    "featureId": {
      "type": "integer"
    },
    "sites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::site.site",
      "mappedBy": "protectedArea"
    },
    "parkActivities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::park-activity.park-activity",
      "mappedBy": "protectedArea"
    },
    "parkFacilities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::park-facility.park-facility",
      "mappedBy": "protectedArea"
    },
    "parkNames": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::park-name.park-name",
      "mappedBy": "protectedArea"
    },
    "hasCampfireBan": {
      "type": "boolean"
    },
    "hasSmokingBan": {
      "type": "boolean"
    },
    "fireZones": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::fire-zone.fire-zone",
      "mappedBy": "protectedAreas"
    },
    "managementAreas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::management-area.management-area",
      "mappedBy": "protectedAreas"
    },
    "hasCampfireBanOverride": {
      "type": "boolean"
    },
    "hasSmokingBanOverride": {
      "type": "boolean"
    },
    "campfireBanRescindedDate": {
      "type": "date"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "safetyInfo": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "specialNotes": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "locationNotes": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "parkContact": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "reservations": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "maps": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "managementPlanning": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "natureAndCulture": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "partnerships": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "purpose": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "reconciliationNotes": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "slug": {
      "type": "string"
    },
    "isDisplayed": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "parkOperationSubAreas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::park-operation-sub-area.park-operation-sub-area",
      "mappedBy": "protectedArea"
    },
    "parkOperation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::park-operation.park-operation",
      "mappedBy": "protectedArea"
    },
    "parkSubPages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::park-sub-page.park-sub-page",
      "mappedBy": "protectedArea"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "parks.seo"
    },
    "publicAdvisories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::public-advisory.public-advisory",
      "mappedBy": "protectedArea"
    },
    "parkPhotos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::park-photo.park-photo"
    }
  }
}

      "enum": [
        "Operation",
        "Reservation",
        "Winter fee"
      ],
      "required": true
    },
    "adminNote": {
      "type": "text"
    },
    "parkOperationSubArea": {
      "type": "relation",
      "relation": "manyToOne",

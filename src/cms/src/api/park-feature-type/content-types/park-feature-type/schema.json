{
  "kind": "collectionType",
  "collectionName": "park_feature_types",
  "info": {
    "singularName": "park-feature-type",
    "pluralName": "park-feature-types",
    "displayName": "Park-feature-type"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "parkFeatureType": {
      "type": "string",
      "required": true
    },
    "featureTypeId": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "closureAffectsAccessStatus": {
      "type": "boolean",
      "default": null
    },
    "campingType": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::camping-type.camping-type"
    },
    "facilityType": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::facility-type.facility-type"
    },
    "parkFeatures": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::park-feature.park-feature",
      "mappedBy": "parkFeatureType"
    },
    "rank": {
      "type": "integer"
    }
  }
}
